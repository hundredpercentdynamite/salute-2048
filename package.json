{
  "name": "salute-2048",
  "version": "0.0.0",
  "private": true,
  "description": "A React clone of 2048 game",
  "main": "src/index.js",
  "homepage": "https://volodyathehurricane.github.io/salut-2048",
  "scripts": {
    "build": "webpack --config scripts/webpack.prod.js",
    "start": "webpack serve --config scripts/webpack.dev.js",
    "start:host": "webpack serve --config scripts/webpack.dev.js --host 0.0.0.0",
    "start:pwa": "npm run build && http-server dist --port=3000",
    "lint": "eslint . --ext .js,.ts,.tsx --max-warnings 0",
    "format": "prettier --write .",
    "tsc": "tsc -p tsconfig.json --noEmit",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "Kleymenov",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "@sberdevices/assistant-client": "^1.0.5",
    "@sberdevices/plasma-icons": "^1.4.0",
    "@sberdevices/plasma-tokens": "^1.1.0",
    "@sberdevices/plasma-ui": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "css-loader": "^5.2.6",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "favicons": "^6.2.1",
    "favicons-webpack-plugin": "^5.0.2",
